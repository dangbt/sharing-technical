    "gatsby-transformer-remark": "^2.6.32",
    "postcss-import": "^12.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "tailwindcss": "^1.1.3"
